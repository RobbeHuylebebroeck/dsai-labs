    "# calculate difference between each index and the previous one, expressed in hours\n",
    "# to_series() converts the index data type to a pandas Series\n",
    "# diff() calculates the difference between each value and the previous one\n",
    "# dt.total_seconds() converts the difference to seconds, and we divide by 3600 to convert to hours\n",
    "air_quality['timespan'] = air_quality.index.to_series().diff().dt.total_seconds() / 3600\n",
    "air_quality.head()"
   ]
